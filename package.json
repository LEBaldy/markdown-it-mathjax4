{
  "name": "markdown-it-mathjax3",
  "version": "5.1.0",
  "description": "Fast math support for markdown-it with MathJax",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:nzt/markdown-it-mathjax3.git"
  },
  "keywords": [
    "markdown",
    "MathJax",
    "math",
    "LaTeX",
    "markdown-it-plugin",
    "markdown-it"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "MIT",
  "dependencies": {
    "mathxyjax3": "^0.5.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "markdown-it": "^14.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:code && npm run build:type",
    "build:code": "tsc",
    "build:type": "tsc -d",
    "format": "prettier --write *.ts *.json"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "index.d.ts",
    "package.json"
  ]
}
